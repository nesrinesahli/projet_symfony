<footer class="footer">
    <div class="footer-secondary">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-md-6">
                    <span class="fz-14">{{ footer.contact }}</span>
                </div>
                <div class="col-md-6">
                    <nav>
                        <ul class="list-unstyled footer__copyright-links d-flex justify-content-end mb-0">
                            {% for link in footer.links %}
                                <li><a href="{{ link.url }}">{{ link.text }}</a></li>
                            {% endfor %}
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</footer>
